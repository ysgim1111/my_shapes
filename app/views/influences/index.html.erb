<div class="container inf_list">
  <div class="row">
    <div class="col-md-12 main_slide">
      <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
          <% @products.where(view_type: Product.view_types[:main]).each_with_index do |product, i| %>
            <li id="carousel_<%= i + 1 %>" data-target="#myCarousel" data-slide-to="<%= i %>" class="<%= i == 0 ? 'active' : '' %>"></li>
          <% end %>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
          <% @products.where(view_type: Product.view_types[:main]).each_with_index do |product, i| %>
          <div class="item <%= i == 0 ? 'active' : '' %>">
            <%= link_to product_path(product) do %>
              <%= image_tag(product.image_url2, alt: product.name, style: "width:100%") %>
            <% end %>
            <div class="carousel-caption invisible">
              <h3><%= product.name %></h3>
              <p><%= product.desc %></p>
            </div>
          </div>
          <% end %>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
          <%= image_tag("slide_arrow_left.png", alt: "") %>
          <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
          <%= image_tag("slide_arrow_right.png", alt: "") %>
          <span class="sr-only">Next</span>
        </a>
      </div>
