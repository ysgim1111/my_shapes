<table class="table table-condensed table-striped">
  <thead>
    <tr>
      <th>상품명</th>
      <th>설명</th>
      <th>가격</th>
      <th>상태</th>
      <th>생성일</th>
    </tr>
  </thead>
  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= link_to product.name, cms_product_path(product) %></td>
        <td><%= product.desc %></td>
        <td><%= product.price %></td>
        <td><%= product.status %></td>
        <td><%= product.created_at.strftime("%Y-%m-%d") %></td>
      </tr>
    <% end -%>
  </tbody>
</table>
